page {
	background-color: #f0f0f0;
}

.container {
	font-family: "Helvetica Neue",Helvetica,STHeiTi,sans-serif;
    line-height: 1.5;
    font-size: 14px;
    color: #000;
    -webkit-user-select: none;
    -webkit-text-size-adjust: none;
    -webkit-tap-highlight-color: rgba(255,255,255,0);
	background: #f0f0f0;
}

action-sheet {
	position: relative;
	z-index: 9999;
}

picker {
	display: block;
}

.pd-b20 {
	padding-bottom: 20px;
}

.mr-b10 {
    margin-bottom: 10px;
}

.br-b0 {
	border-bottom: 0px!important;
}

.br-tl1 {
	border-radius: 3px 0 0 3px;
}

.br-rb1 {
	border-radius: 0 3px 3px 0;
}

.ui-nowrap {
	font-size: 16px;
    line-height: 24px;
    max-width: 100%;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    display: block;
}

.dis-inline {
    display: inline-block!important;
}

.fs-14 {
    font-size: 14px;
}